            <a>cd</a>
            <b>checkData</b>
        </field>
    </mapping>

    <mapping>
        <class-a>cz.nic.xml.epp.contact_1.CheckType</class-a>
        <class-b>fred.client.data.check.contact.ContactCheckData</class-b>
        <field>
            <a>id.value</a>
            <b>id</b>
        </field>
        <field>
            <a>id.avail</a>
            <b>avail</b>
        </field>
        <field>
            <a>reason.value</a>
            <b>reason</b>
        </field>
